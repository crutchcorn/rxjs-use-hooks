{
  "name": "rxjs-use-hooks",
  "version": "0.0.0-development",
  "description": "Simple-to-use React Hooks for RxJS interop",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crutchcorn/rxjs-use-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/crutchcorn/rxjs-use-hooks/issues"
  },
  "homepage": "https://github.com/crutchcorn/rxjs-use-hooks#readme",
  "type": "commonjs",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "prepare": "husky install",
    "test": "vitest",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rxjs": "^7.8.1",
    "semantic-release": "^21.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.1"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0 || ^18.0",
    "rxjs": ">=6.0.0"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/rxjs-use-hooks.mjs",
  "main": "./dist/rxjs-use-hooks.cjs",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/rxjs-use-hooks.mjs",
      "require": "./dist/rxjs-use-hooks.cjs",
      "default": "./dist/rxjs-use-hooks.cjs"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">= 99999.0.0",
    "pnpm": ">= 8.0.0"
  },
  "lint-staged": {
    "*{.js,.jsx,.ts,.tsx}": "eslint --fix"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
